{
  "dashboard": {
    "title": "Victor AI - Production Metrics",
    "description": "Production monitoring dashboard for Victor AI coding assistant",
    "tags": ["victor", "ai", "production"],
    "timezone": "UTC",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(victor_request_total[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 2,
        "title": "Request Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(victor_request_latency_ms_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(victor_request_latency_ms_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(victor_request_latency_ms_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(victor_request_errors_total[5m]) / rate(victor_request_total[5m]) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [5], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Error Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 4,
        "title": "Active Requests",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "victor_active_requests",
            "legendFormat": "Active Requests",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "color": {"mode": "thresholds"}
          }
        }
      },
      {
        "id": 5,
        "title": "Tool Execution Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(victor_tool_duration_ms_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(victor_tool_duration_ms_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "title": "Tool Call Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(victor_tool_calls_total[5m])",
            "legendFormat": "Tool Calls/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 7,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "victor_cache_hits_total / (victor_cache_hits_total + victor_cache_misses_total) * 100",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "green"}
              ]
            },
            "mappings": []
          }
        }
      },
      {
        "id": 8,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 24},
        "targets": [
          {
            "expr": "victor_memory_usage_bytes",
            "legendFormat": "Memory",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 9,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "victor_cpu_usage_percent",
            "legendFormat": "CPU %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 10,
        "title": "Service Uptime",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 24},
        "targets": [
          {
            "expr": "victor_uptime_seconds",
            "legendFormat": "Uptime",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "dtdurations",
            "mappings": [],
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Tasks Completed",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "rate(victor_tasks_completed_total[5m])",
            "legendFormat": "Tasks/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 12,
        "title": "Total Cost",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "victor_total_cost_usd",
            "legendFormat": "Cost (USD)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 13,
        "title": "Token Usage Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "rate(victor_tokens_used_total[5m])",
            "legendFormat": "Tokens/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 14,
        "title": "Errors by Type",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "topk(10, victor_errors_total)",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      }
    ]
  }
}
