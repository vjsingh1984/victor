# Victor API Keys Configuration
# ==============================
#
# Copy this file to ~/.victor/api_keys.yaml and fill in your API keys.
#
# SECURITY:
#   - This file should be stored OUTSIDE your code repository
#   - Set file permissions to 0600 (owner read/write only):
#     chmod 600 ~/.victor/api_keys.yaml
#   - NEVER commit API keys to version control
#
# PRIORITY:
#   Environment variables take precedence over this file.
#   For example, ANTHROPIC_API_KEY env var overrides the anthropic key below.
#
# SETUP:
#   mkdir -p ~/.victor
#   cp examples/api_keys.yaml.example ~/.victor/api_keys.yaml
#   chmod 600 ~/.victor/api_keys.yaml
#   # Edit ~/.victor/api_keys.yaml and add your keys

api_keys:
  # ===========================================================================
  # ANTHROPIC - Claude Models
  # ===========================================================================
  # Models: claude-3-opus, claude-3-sonnet, claude-3-haiku, claude-sonnet-4
  # Get key: https://console.anthropic.com/settings/keys
  # Env var: ANTHROPIC_API_KEY
  anthropic: "sk-ant-api03-YOUR_KEY_HERE"

  # ===========================================================================
  # OPENAI - GPT Models
  # ===========================================================================
  # Models: gpt-4o, gpt-4-turbo, gpt-4, gpt-3.5-turbo
  # Get key: https://platform.openai.com/api-keys
  # Env var: OPENAI_API_KEY
  openai: "sk-YOUR_KEY_HERE"

  # ===========================================================================
  # GOOGLE - Gemini Models
  # ===========================================================================
  # Models: gemini-2.0-flash-exp, gemini-1.5-pro, gemini-1.5-flash
  # Get key: https://aistudio.google.com/apikey
  # Env var: GOOGLE_API_KEY
  google: "AIzaSy-YOUR_KEY_HERE"

  # ===========================================================================
  # XAI - Grok Models
  # ===========================================================================
  # Models: grok-beta, grok-2
  # Get key: https://console.x.ai/
  # Env var: XAI_API_KEY
  xai: "xai-YOUR_KEY_HERE"

  # ===========================================================================
  # MOONSHOT - Kimi K2 Models
  # ===========================================================================
  # Models: kimi-k2-thinking, kimi-k2-thinking-turbo, kimi-k2-instruct
  # Get key: https://platform.moonshot.cn/console/api-keys
  # Env var: MOONSHOT_API_KEY
  # Features: 256K context, reasoning traces, native tool calling
  moonshot: "sk-YOUR_KEY_HERE"

  # ===========================================================================
  # DEEPSEEK - DeepSeek-V3 Models
  # ===========================================================================
  # Models: deepseek-chat (with tools), deepseek-reasoner (with CoT, no tools)
  # Get key: https://platform.deepseek.com/api_keys
  # Env var: DEEPSEEK_API_KEY
  # Features: 128K context, very cost-effective (~10-30x cheaper than OpenAI)
  # Note: deepseek-reasoner does NOT support function calling
  deepseek: "sk-YOUR_KEY_HERE"

  # ===========================================================================
  # LOCAL PROVIDERS - No API Keys Required
  # ===========================================================================
  # The following providers run locally and don't need API keys:
  #   - ollama: http://localhost:11434
  #   - lmstudio: http://localhost:1234
  #   - vllm: http://localhost:8000
  #
  # Configure their endpoints in ~/.victor/profiles.yaml instead.


# ===========================================================================
# USAGE EXAMPLES
# ===========================================================================
#
# 1. Using environment variables (recommended for CI/CD):
#    export ANTHROPIC_API_KEY="sk-ant-api03-..."
#    export DEEPSEEK_API_KEY="sk-..."
#    victor chat -p claude
#
# 2. Using this file:
#    # Edit this file with your keys, then:
#    victor chat -p deepseek
#
# 3. Check configured providers:
#    python -c "from victor.config.api_keys import get_configured_providers; print(get_configured_providers())"
#
# 4. Verify a specific key is loaded:
#    python -c "from victor.config.api_keys import get_api_key; print('Key loaded' if get_api_key('deepseek') else 'No key')"
